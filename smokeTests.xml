<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Tests" parallel="classes" thread-count="2">
    <parameter name="browser" value="chrome"/>
    <test name="Login Smoke tests">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <packages>
            <package name="Tests"/>
        </packages>
    </test>
</suite>